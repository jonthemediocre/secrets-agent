# AI Gateway Configuration
PORT=3001

# Vault Integration (CRITICAL - Must authenticate through Secrets Vault)
VAULT_BASE_URL=http://localhost:3000
VAULT_ACCESS_TOKEN=your-vault-access-token-here

# Redis Configuration (for rate limiting and caching)
REDIS_URL=redis://localhost:6379

# Security
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8081,http://localhost:3002
JWT_SECRET=your-jwt-secret-here

# Logging
LOG_LEVEL=info

# Rate Limiting
RATE_LIMIT_POINTS=100
RATE_LIMIT_DURATION=60

# Required Vault Secrets (stored in Secrets Vault):
# VANTA_AI_API_KEY=your-vanta-api-key
# VANTA_AI_BASE_URL=https://api.vanta.ai
# VANTA_AI_PROJECT_ID=your-vanta-project-id 