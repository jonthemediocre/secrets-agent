{
  "name": "secrets-agent",
  "version": "1.3.4",
  "private": true,
  "description": "Advanced Secrets Management Platform with AI-powered governance",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:full": "tsc && next build",
    "start": "next start",
    "dev:web": "next dev",
    "dev:api": "echo \"✅ API routes are handled by Next.js - no separate server needed\"",
    "clean": "rimraf dist .next",
    "lint": "next lint",
    "lint:check": "next lint",
    "type-check": "tsc --noEmit",
    "cli": "tsx src/cli/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:api": "jest --config jest.config.cjs --testPathPattern=api",
    "test:components": "jest --config jest.config.cjs --testPathPattern=components",
    "test:integration": "jest --config jest.config.cjs --testPathPattern=integration",
    "test:ci": "jest --config jest.config.cjs --coverage --watchAll=false",
    "test:security": "npm audit && npm run test:coverage",
    "test:all": "npm run lint && npm run type-check && npm run test:ci",
    "mcp:validate": "tsx scripts/validate-mcp.ts",
    "mcp:check": "npm run mcp:validate && echo '✅ MCP compliance verified'",
    "uap:validate": "npm run mcp:validate",
    "uap:check": "npm run type-check && npm run mcp:validate",
    "uap:cli": "tsx scripts/uap-cli.ts",
    "uap": "npm run uap:cli",
    "agent:validate": "npm run uap:check",
    "health:check": "node scripts/health-check.js",
    "migrate:up": "docker-compose -f docker-compose.database.yml --profile migration up vanta-migrations",
    "migrate:status": "docker exec vanta-postgres-primary psql -U vanta_admin -d vanta_production -c 'SELECT * FROM vanta.migrations ORDER BY applied_at DESC LIMIT 10;'",
    "migrate:rollback": "node scripts/rollback-migration.js",
    "database:start": "docker-compose -f docker-compose.database.yml up -d",
    "database:stop": "docker-compose -f docker-compose.database.yml down",
    "database:logs": "docker-compose -f docker-compose.database.yml logs -f",
    "database:reset": "docker-compose -f docker-compose.database.yml down -v && docker-compose -f docker-compose.database.yml up -d",
    "backup:create": "docker-compose -f docker-compose.database.yml --profile backup up vanta-backup",
    "backup:restore": "node scripts/restore-backup.js",
    "monitoring:start": "docker-compose -f docker-compose.database.yml --profile monitoring up -d",
    "benchmark:database": "node scripts/benchmark-database.js",
    "security:scan": "docker run --rm -v $(pwd):/app -w /app securecodewarrior/docker-security-scan",
    "production:deploy": "docker-compose -f docker-compose.production.yml up -d",
    "production:status": "docker-compose -f docker-compose.production.yml ps"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.9.1",
    "@heroicons/react": "^2.2.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.9.0",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.2.7",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^11.10.0",
    "chalk": "^5.3.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fast-glob": "^3.3.3",
    "framer-motion": "^12.16.0",
    "glob": "^11.0.2",
    "helmet": "^7.1.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.513.0",
    "next": "^14.0.0",
    "next-auth": "^4.24.11",
    "node-cron": "^4.1.0",
    "node-fetch": "^2.7.0",
    "prisma": "^6.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.15.3",
    "sqlite3": "^5.1.7",
    "tailwind-merge": "^3.3.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.11.16",
    "@types/node-cron": "^3.0.11",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.0-beta.3",
    "postcss": "^8.5.4",
    "rimraf": "^5.0.0",
    "supertest": "^7.1.1",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.7.1",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
