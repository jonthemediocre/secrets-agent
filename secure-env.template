# Template environment variables for Secrets Agent with secure storage
# Copy this file to .env and replace with real values

# API key for accessing the API
API_KEY=your_api_key_here

# OpenAI API key if using AI features
OPENAI_API_KEY=your_openai_api_key_here

# Master password for encrypted secrets
# IMPORTANT: Use a strong password in production!
MASTER_PASSWORD=your_master_password_here

# Set to true to enable secure encrypted storage
SECURE_STORAGE=true 